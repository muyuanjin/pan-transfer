  "scripts": {
    "dev": "vite",
    "typecheck": "vue-tsc --noEmit -p tsconfig.app.json",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "format:silent": "prettier --write . --log-level=warn",
    "build": "npm run typecheck && vite build --mode production",
    "test": "vitest run",
    "e2e": "playwright test && node -e \"require('node:fs').rmSync('test-results',{recursive:true,force:true})\"",
    "check": "npm run format:silent && npm run typecheck && npm run lint:quiet && npm run build && npm run test && npm run e2e",
    "test:watch": "vitest",
    "preview": "vite preview",
    "lint:quiet": "eslint . --quiet"
  },
