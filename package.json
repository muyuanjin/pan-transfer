    "postcss-nesting": "^12.1.0",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3",
    "vite": "^7.1.12",
    "vite-plugin-web-extension": "^4.5.0",
    "vitest": "^2.1.4",
